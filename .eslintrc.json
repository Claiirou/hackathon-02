{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/babel", "next/core-web-vitals", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": { "react/no-unescaped-entities": "off" }
}
